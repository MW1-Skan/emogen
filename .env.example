# OpenAI API key used by the Genkit flow
OPENAI_API_KEY=sk-your-key-here

# Optional: override the model (defaults to gpt-4o-mini)
OPENAI_EMOJI_MODEL=gpt-4o-mini

# Port used by npm run genkit:dev
GENKIT_PORT=4000

# Comma-separated list of origins allowed to hit the flow locally
EMOGEN_ALLOWED_ORIGINS=http://localhost:4200
